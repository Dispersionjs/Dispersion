{
  "name": "dispersion",
  "version": "1.0.0",
  "description": "Electron based interface for ipfs",
  "main": "main.js",
  "scripts": {
    "test": "mocha test",
    "start": "electron .",
    "clean": "rm -rf ./dist",
    "clean:osx": "rm -rf ./dist/osx",
    "pack": "npm run clean:osx && electron-packager . \"EA Todo\" --out=dist/osx --platform=darwin --arch=x64 --version=0.36.2 --icon=assets/osx/logo.icns --ignore=dist --ignore=assets --ignore=builder.json --ignore=bower.json --ignore=README.md --ignore=.gitignore --ignore=preview.png",
    "build": "npm run pack:osx && electron-builder \"dist/osx/EA Todo-darwin-x64/EA Todo.app\" --platform=osx --out=\"dist/osx\" --config=builder.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dispersionjs/Dispersion.git"
  },
  "keywords": [
    "electron",
    "ipfs",
    "ipns",
    "dispersion",
    "webhosting",
    "p2p",
    "peer-to-peer",
    "distributed-web",
    "dag",
    "dht"
  ],
  "author": "Dispersion",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dispersionjs/Dispersion/issues"
  },
  "homepage": "http://dispersion.io",
  "dependencies": {
    "angular": "^1.5.8",
    "dialog": "^0.2.0",
    "electron": "^1.4.2",
    "electron-builder": "^7.11.4",
    "electron-json-storage": "^2.0.0",
    "electron-packager": "^8.1.0",
    "file-type": "^3.8.0",
    "fs": "0.0.1-security",
    "fs-extra": "^0.30.0",
    "hammerjs": "^2.0.8",
    "https": "^1.0.0",
    "jquery": "^3.1.1",
    "mime": "^1.3.4",
    "read-chunk": "^2.0.0",
    "remote": "^0.2.6",
    "request": "^2.75.0",
    "username": "^2.2.2"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.8",
    "benv": "^3.1.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "describe-it": "^1.7.0",
    "electron-mocha": "^3.1.1",
    "expect.js": "^0.3.1",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-electron": "^5.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "spectron": "^3.4.0"
  }
}
